{
  "extends": "airbnb-base",
  "env": {
    "browser": true
  },
  "parser": "@babel/eslint-parser",
  "rules": {
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": [
          "gulpfile.js",
          "gulp/**/*"
        ]
      }
    ],
    "import/extensions": [
      "error",
      "always",
      {
        "ignorePackages": true
      }
    ]
  }
}
